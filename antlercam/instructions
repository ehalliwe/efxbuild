export AWS_ACCESS_KEY_ID=AKIATQPD64A5TW3LSUTY

gst-inspect-1.0 kvssink

gst-launch-1.0 libcamerasrc \
  ! video/x-raw,width=640,height=480,framerate=2/1,format=I420 \
  ! videoconvert \
  ! x264enc speed-preset=ultrafast tune=zerolatency byte-stream=true bframes=0 key-int-max=2 \
  ! video/x-h264,level='(string)4' \
  ! h264parse \
  ! video/x-h264,stream-format=avc,alignment=au,width=640,height=480,framerate=2/1 \
  ! kvssink stream-name="antlerCam"

  https://us-west-2.console.aws.amazon.com/kinesisvideo/home?region=us-west-2#/streams/streamName/antlerCam